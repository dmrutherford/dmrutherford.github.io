---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	
	<title type="text" xml:lang="en">forkThis, by D M Rutherford</title>
	<link type="application/atom+xml" href="http://dmrutherford.github.com/feed.xml" rel="self"/>
 	<link type="text" href="http://dmrutherford.github.com" rel="alternate"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<guid>http://dmrutherford.github.com</guid>
	<author>
		<name>D M Rutherford</name>
	</author>
	
	{% for post in site.posts limit:10 %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://dmrutherford.github.com{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<guid>http://dmrutherford.github.com{{ post.id }}</guid>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>